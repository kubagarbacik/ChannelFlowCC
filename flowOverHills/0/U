FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (5.5273 0 0);

boundaryField
{
    inlet
    {
	type            cyclic;
    }

    outlet
    {
	type            cyclic;
    }

    wall
    {
        type            noSlip;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}


